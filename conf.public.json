{
  "source": {
    "include": ["lib/"]
  },
  "opts": {
    "access": "public",
    "recurse": true,
    "destination": "docs_public/"
  },
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-class-properties",
      "external-helpers"
    ]
  }
}
